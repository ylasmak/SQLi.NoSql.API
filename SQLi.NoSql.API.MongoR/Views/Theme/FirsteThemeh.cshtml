@model SQLi.NoSql.API.MongoR.Models.ReportModelTheme
<div class="">
    <div class="page-title">
        <div class="title_left">
            <h3> Title exemple in document file    <small> Rapport </small> </h3>
        </div>

        <div class="title_right">
            <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
                <div class="input-group">
                    <input type="text" class="form-control" placeholder="Search for...">
                    <span class="input-group-btn">
                        <button class="btn btn-default" type="button">Go!</button>
                    </span>
                </div>
            </div>
        </div>
    </div>

    <div class="clearfix"></div>
    <div class="row">
        <div class="col-md-12">
            <div class="x_panel">
                <div class="x_title">
                    <h2>Dossiers</h2>
                    <ul class="nav navbar-right panel_toolbox">
                        <li>
                            <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                        </li>
                        
                    </ul>
                    <div class="clearfix"></div>
                </div>
                <div class="x_content">
                    <div class="row sortableUI">
                     <div class="col-md-4 col-sm-4 col-xs-6">
                         @Html.Partial("~/Views/Theme/_FormView.cshtml", Model.CurrentReport)
                     </div>
                        <div class="col-md-4 col-sm-4 col-xs-6">
                            @Html.Partial("~/Views/Theme/_GrapheView.cshtml", Model.CurrentReport)
                        </div>
                        <div class="col-md-4 col-sm-4 col-xs-6">
                            @Html.Partial("~/Views/Theme/_ResultTableView.cshtml", Model.CurrentReport)
                        </div>
                  
                        <div class="col-md-12 col-sm-12 col-xs-12">
                            @Html.Partial("~/Views/Theme/_LogView.cshtml",Model.CurrentReport)
                        </div>
                        </div>
                    </div>
            </div>
        </div>
    </div>

    <div class="clearfix"></div>
 </div>
@Scripts.Render("~/bundles/chart")

