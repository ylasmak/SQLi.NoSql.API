@model SQLi.NoSql.API.MongoR.Models.ReportModelTheme
 <div class="x_panel">
        <div class="x_title">
            <h2>Log Transaction  <small>Custom design</small></h2>
            <ul class="nav navbar-right panel_toolbox">
                                
                <li>
                    <a class="close-link"><i class="fa fa-close"></i></a>
                </li>
            </ul>
            <div class="clearfix"></div>
        </div>

        <div class="x_content">

         

            <div class="table-responsive">
                <table class="table table-striped jambo_table bulk_action">
                    <thead>
                        <tr class="headings">
                           
                            <th class="column-title">Execution Log </th>
                           
                    </thead>

                    <tbody>
                        @if (Model.CurrentReport.Log != null)
                        {
                            foreach (var log in Model.CurrentReport.Log)
                            {
                            <tr class="even pointer">

                                <td class=" ">@log</td>

                            </tr>
                            }
                        }



                    </tbody>
                </table>
            </div>
        </div>
</div>
